# Session Configuration
SESSION_SECRET=your_session_secret_here

# Stratix AI Integration
STRATIX_API_URL=http://localhost:3001
STRATIX_API_KEY=your_stratix_api_key_here

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/stratix_ai_dev?schema=public"

# Ngrok Configuration
NGROK_URL=https://your-ngrok-url.ngrok.io
