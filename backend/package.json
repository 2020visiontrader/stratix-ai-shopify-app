{
  "name": "stratix-ai-shopify-app-backend",
  "version": "1.0.0",
  "description": "Backend for Stratix AI Shopify App",
  "main": "dist/init.js",
  "scripts": {
    "start": "node dist/init.js",
    "dev": "ts-node-dev --respawn --transpile-only src/init.ts",
    "dev:simple": "node simple-server.js",
    "dev:ngrok": "node ngrok-tunnel.js",
    "dev:with-tunnel": "concurrently \"npm run dev:simple\" \"npm run dev:ngrok\"",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ngrok/ngrok": "^1.5.1",
    "@shopify/shopify-api": "^11.13.0",
    "@shopify/shopify-app-express": "^5.0.18",
    "@supabase/supabase-js": "^2.39.0",
    "@types/multer": "^1.4.13",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "llamaindex": "^0.11.8",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.1.0",
    "nodemailer": "^7.0.3",
    "openai": "^4.20.0",
    "pdf-parse": "^1.1.1",
    "rate-limiter-flexible": "^7.1.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^9.1.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
